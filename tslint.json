{
  "defaultSeverity":"none",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [
      false,
      {
          "import-sources-order": "lowercase-last",
          "named-imports-order": "lowercase-first"
      }
    ]
  }
}